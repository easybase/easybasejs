{
  "name": "easybasejs",
  "version": "2.0.0",
  "description": "Broswer and Node.js package for use with EasyBase.io",
  "main": "dist/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js && npx webpack",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "npx babel src --out-dir dist --extensions \".ts,.tsx\" --source-maps inline"
  },
  "files": [
    "dist/index.js",
    "dist/bundle.js",
    "src/index.js"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/easybase/easybasejs.git"
  },
  "author": "EasyBase",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/easybase/easybasejs/issues"
  },
  "homepage": "https://github.com/easybase/easybasejs#readme",
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "eslint": "^7.4.0",
    "source-map-loader": "^1.1.1",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "dependencies": {
    "@glidejs/glide": "^3.4.1",
    "axios": "^0.19.2"
  }
}
